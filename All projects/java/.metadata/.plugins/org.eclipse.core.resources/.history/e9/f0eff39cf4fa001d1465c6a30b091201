package org.BufferZero;

public class ArrayREverse {
public static void main(String[] args) {
	int arr[]= {12,23,45,67};
	int i=0, j=arr.length-1;
//	for(int i=0;i<arr.length;i++) {
	
		while(i<j) {
			int temp=arr[i];
			arr[i]=arr[j];
			arr[j]=temp;
			
			
		}
		i++;
		j--;
		
		System.out.println(arr[i]+" ");
	}
}

