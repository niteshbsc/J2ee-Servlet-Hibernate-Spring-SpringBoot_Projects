class PrimeOrNotBoolean 
{
	public static void main(String[] args) 
	{
		boolean prime=isPrime(1);
		System.out.println(prime);
	}
	static boolean isPrime(int num)
    {
        
       for(int i=2;i<=num/2;i++)
       {
           if(num%i==0)
               return  false;
       }
       return true;
    }
}